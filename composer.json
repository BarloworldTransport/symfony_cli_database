{
	"description": "Access and output database information from the CLI",
    "homepage": "https://github.com/noondaysun/symfony_cli_database",
    "license": "GNU GPL v2.0",
    "name": "BWT/symfony_cli_databases",
    "autoload": {
        "psr-4": {
            "NoondaySun\\Symfony_cli_database\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "NoondaySun\\Symfony_cli_database\\": "tests" 
        }
    },
    "require": {
    	"php": "^7.0",
    	"symfony/console": "^3.3",
    	"zendframework/zend-config": "^3.1"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "mayflower/php-codebrowser": "~1.1",
        "pdepend/pdepend" : "2.3.2",
        "phploc/phploc": "*",
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": "~5.1",
        "phpunit/phpunit-selenium": ">=1.2",
        "sebastian/phpcpd": "*",
        "squizlabs/php_codesniffer": "~2.5.0",
        "phing/phing": "2.*"
    },
    "type": "package"
}
